{
  "version": "1.4.2",
  "name": "alpinejs-app",
  "description": "Simple SPA plugin for Alpinejs",
  "repository": {
        "type": "git",
        "url": "https://github.com/vseryakov/alpinejs-app.git"
  },
  "author": "Vlad Seryakov",
  "license": "MIT",
  "keywords": [ "Alpine JS", "Alpine JS SPA" ],
  "module": "dist/app.mjs",
  "unpkg": "dist/app.min.js",  
  "files": [
    "dist/",
    "src/",
    "scripts/",
    "builds/"
  ],
  "scripts": {
    "demo": "sh scripts/demo.sh",
    "build": "node scripts/build.js",
    "devbuild": "node scripts/build.js",
    "watch": "cd examples && npm run watch"
  },
  "devDependencies": {
    "esbuild": "^0.25.0"
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    }
  }
}
